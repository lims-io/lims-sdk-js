{
  "name": "@lims.io/app",
  "version": "0.1.0",
  "main": "dist/index.js",
  "description": "The primary entrypoint to the Firebase JS SDK",
  "private": true,
  "engines": {
    "node": ">=6.11.5",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js --ext yml,yaml,js,json --exec \"node -r babel-register\"",
    "prebuild": "yarn clean:dist && mkdir -p dist",
    "build": "babel -w src -s -D -d dist > /dev/null",
    "clean:dist": "rimraf dist",
    "clean:src": "rimraf src",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "@lims.io/auth": "^0.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "jsonwebtoken": "^8.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-watch": "^3.1.3",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2"
  }
}
